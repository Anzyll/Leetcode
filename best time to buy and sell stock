var maxProfit = function (prices) {
  let maxSum = 0;
  let left = 0;
  let right = 1;
  while (right < prices.length) {
    if (prices[left] < prices[right]) {
      let curSum = prices[right] - prices[left];
      if (curSum > maxSum) {
        maxSum = curSum;
      }
    } else {
      left = right;
    }
    right++;
  }
  return maxSum;
};
